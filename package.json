{
  "name": "ext-to-regex",
  "description": "Create a regex for matching file extensions.",
  "version": "0.1.0",
  "homepage": "https://github.com/jonschlinkert/ext-to-regex",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/ext-to-regex",
  "bugs": {
    "url": "https://github.com/jonschlinkert/ext-to-regex/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
