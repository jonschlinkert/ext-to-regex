    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "dotfile-regex",
        "ext-regex",
        "filename-regex"
      ]
    },
    "reflinks": [
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
